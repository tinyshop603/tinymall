page {
  height: 100%;
}

.container {
  background: #fff;
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.search{
    background: #F4F4F4;
    border-radius: 10rpx;
    padding: 0 20rpx;
    height: 60rpx;
    line-height: 60rpx;
    margin: 0 20rpx;
}
.search .icon{
  float: left;
  width: 38rpx;
  height: 38rpx;
  margin-top: 10rpx;
  background: url(data:image/png;base64,) no-repeat;
}
.search input{
  float: left;
  height: 100%;
  width: 600rpx;
  margin-left:20rpx;
}
.line{
  margin-top:30rpx;
  height: 2rpx;
  background-color: #F4F4F4;
}
.title{
  margin: 20rpx;
  font-size: 20rpx;
  color: #ccc;
}
.nearby-address .item{
  /* height: 80rpx; */
  margin: 20rpx;
  border-bottom: 1px solid #ccc;
}

.nearby-address .l{
  width: 450rpx;
  float: left;
  padding: 20rpx;
  overflow: hidden;
}


.nearby-address .r{
  float:right;
  padding: 20rpx;
  color: #7A7A7A;
}

.no-res{
  margin: auto;
}
.clear{
  clear:both;
}