page{
    height: 100%;
    width: 100%;
    background: #fff;
}
.container{
    background: #f4f4f4;
    height: auto;
    overflow: hidden;
    width: 100%;
}
.profile-info{
    width: 100%;
    height: 550rpx;
    display: block;
    /* vertical-align: middle;
    text-align: center; */
    /* flex-wrap: wrap;
    align-items: center;
    justify-content: flex-start; */
    padding: 0 30.25rpx;
    background-size:100% 100%;
    -moz-background-size:100% 100%;
    background-image: url("data:image/png;base64,");
}

.profile-info .picture{
  width: 100%;
  height: 220rpx;
  padding-top: 70rpx;
  text-align: center;
}

.profile-info .picture .avatar{
    height: 150rpx;
    width: 150rpx;
    border-radius: 50%;
}

.profile-info .info{
    text-align: center;
    height: 80rpx;
    padding-top: 25rpx;
}

.profile-info .name{
    display: block;
    height: 45rpx;
    line-height: 45rpx;
    color: #000;
    font-size: 37.5rpx;
}

.profile-info .order {
  height: 80rpx;
  padding-top: 25rpx;
  padding-bottom: 10rpx;
  border-bottom: solid #ccc 1rpx;
}

.profile-info .order .l{
  float: left;
  font-size: 30rpx;
}

.profile-info .order .r{
  float: right;
  font-size: 25rpx;
  color: #888;
  margin-top: 8rpx;
}

.r-arrow{
  float: right;
  width: 32rpx;
  height:32rpx;
  margin-top: 15rpx;
  margin-left: 5rpx;
  background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAUAAAAJCAYAAAD6reaeAAAAZklEQVQYV03MsRFAERBF0bcVyNWgBA3omdxIGKlIFUZi/yzzjReeu9AYg+ecUErhH9VaOaUE5xy01ttprcUhBPTebyA+wxs2ypM3XJSQc4b8f7GUghgjrLUHXzDGgFpr7L3fFwKyD5OkU7ZWfkxvAAAAAElFTkSuQmCC) no-repeat;
}

.profile-info .orderDetail {
  display: flex;
  text-align: center;
  padding-top: 20rpx;
}
.profile-info .orderDetail view{
    width: 33.33%;
}
.profile-info .orderDetail image{
    height: 80rpx;
    width: 80rpx;
}
.profile-info .orderDetail text{
    display: block;
}

.user-menu{
    margin-top: 15rpx;
    width: 100%;
    height: auto;
    overflow: hidden;
    background: #fff;
}

.user-menu .item{
    width: 100%;
    height: 98rpx;
    border-bottom: 1px solid rgba(0,0,0,.15);
    padding-top: 24rpx;
}

.user-menu .item .a{
  /* display: flex; */
  width: 100%;
  height: 100%;
  /* flex-direction: column; */
  /* align-items: center;
  justify-content: center; */
