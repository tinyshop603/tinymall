.catalog {
  flex: 1;
  width: 100%;
  background: #fff;
  display: flex;
  border-top: 1px solid #fafafa;
}

.catalog .nav {
  background: #f7f7f7;
  width: 162rpx;
  height: 100%;
}

.catalog .nav .item {
  text-align: center;
  line-height: 50rpx;
  width: 162rpx;
  height: auto; 
  max-height: 180rpx;
    padding:20rpx 0;
  color: #242531;
  font-size: 28rpx;
  /* border-left: 6rpx solid #fff; */
}

.catalog .nav .item.active {
  color: #242531;
  font-weight: bolder;
  font-size: 35rpx;
  background: #fff;
  border-left: 4rpx solid #FDDE77;
  /* border-left: 6rpx solid #ab2b2b; */
}
.catalog .cur-goods{
  height: 50rpx;
}
.catalog .cate {
  /* border-left: 1px solid #fafafa; */
  flex: 1;
  height: 100%;
  padding: 0 30rpx 0 30rpx;
}

.catalog .cate .goods-title{
    padding: 16rpx 20rpx 6rpx 20rpx;
}

.catalog .cate .goods-title .txt{
    width: auto;
    border-bottom: 1px solid #FDDE77;
}

.listItem1{
	width:100%;
	box-sizing: border-box;
	padding:16rpx 20rpx;
	min-height:222rpx;
	position: relative;
    background-color: #fff;
    margin: 0;
    /* padding: .4rem .266667rem; */
    margin-bottom: 1px;
    display: flex;
    overflow: hidden;
}
.listItem1 .food-tit .addBtn text{
  font-size: 38rpx;
  color:#f60;
}

.notOnSale{
  width:100%;
  z-index:9999;
  top:0;
  left:0;
  opacity:0.3; 
}
.notOnSale .food-tit .addBtn text{
  color:#666;
}
.notOnSale .food-tit .addBtn .btn{
  background: #fff;
}


.itemImg{
	width:170rpx;
	height:170rpx;
	box-sizing: border-box;
	border-radius: 10px;
	overflow: hidden;
	float:left;
}

.food-tit{
	width:349rpx;
	height:163rpx;
	box-sizing: border-box;
	padding-left:15rpx;
}
.food-tit .txt{
  width:100%;
  height: 113rpx;
}

.food-tit .addBtn{
	width:100%;
  height: 50rpx;
}
