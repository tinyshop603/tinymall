page {
  height: 100%;
}

.container {
  background: #f9f9f9;
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.toolBar{
    height: 60rpx;
    line-height: 58rpx;
    margin: 0 20rpx;
}

.toolBar .location{
  float: left;
  width: 80%;
  height: 100%;
  
}
.location .icon{
    background: url(data:image/png;base64,) center no-repeat;
    background-size: 100%;
    width: 28rpx;
    height: 34rpx;
    display: inline-block;
    vertical-align: top;
    margin-top: 15rpx;
}
.location .txt{
  margin-left: 20rpx;
  display: inline-block;
  overflow: hidden;
}

.toolBar .search {
  float: right;
  width: 20%;
  height: 100%;
}

.search .input {
  float: right;
  width: 28rpx;
  height: 100%;
  
}
